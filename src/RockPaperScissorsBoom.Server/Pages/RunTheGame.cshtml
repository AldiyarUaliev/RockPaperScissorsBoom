@page
@model RunTheGameModel

@{
    ViewData["Title"] = "RunTheGame";
}

<h2>Bot Results</h2>

<ul>
    @foreach (var botRank in Model.BotRankings)
    {
        <li>@botRank.Rank) @botRank.BotName: @botRank.Wins-@botRank.Losses</li>
    }
</ul>

<h3>Full Game Results</h3>


